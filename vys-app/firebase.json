{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "dataconnect": {
    "source": "dataconnect",
    "schema": "dataconnect/schema/schema.gql",
    "connector": "dataconnect/connector/connector.yaml"
  }
}
